{
  "name": "memify",
  "version": "1.0.0",
  "main": "src/server/index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "COMPOSER_URL=http://localhost:1234 micro-dev",
    "dev:client": "parcel src/client/index.html",
    "dev:proxy": "micro-proxy -r local-proxy-rules.json -p 9000",
    "dev": "run-p dev:*",
    "build": "parcel build src/client/index.html",
    "now-build": "yarn build",
    "lint": "eslint src --ext .js,.vue .",
    "deploy": "now `[ $NOW_TOKEN ] && echo -t $NOW_TOKEN`",
    "deploy:production": "yarn deploy --target production"
  },
  "browserslist": [
    "since 2017-06"
  ],
  "dependencies": {
    "atob": "2.1.2",
    "btoa": "1.2.1",
    "chrome-aws-lambda": "1.16.1",
    "konva": "^3.2.6",
    "micro": "^9.3.4",
    "microrouter": "3.1.3",
    "puppeteer-core": "1.16.0",
    "query-string": "6.5.0",
    "validator": "10.11.0",
    "vue": "2.6.10",
    "vue-router": "3.0.6",
    "vue2-storage": "4.0.5",
    "vuetify": "1.5.14",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-tyom": "2.4.0",
    "micro-dev": "^3.0.0",
    "micro-proxy": "1.1.0",
    "now": "15.2.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.3",
    "puppeteer": "1.16.0",
    "vue-hot-reload-api": "2.3.3",
    "vue-template-compiler": "2.6.10"
  }
}
